{
  "mcpServers": {
    "typescript-sdk": {
      "type": "stdio",
      "command": "node",
      "args": ["path/to/ts-sdk-server.js"],
      "env": {}
    },
    "github": {
      "type": "stdio",
      "command": "node",
      "args": ["path/to/server-github"],
      "env": {
        "GITHUB_TOKEN": "..."
      }
    },
    "puppeteer": {
      "type": "stdio",
      "command": "node",
      "args": ["path/to/server-puppeteer"],
      "env": {}
    },
    "slack": {
      "type": "stdio",
      "command": "node",
      "args": ["path/to/server-slack"],
      "env": {
        "SLACK_TOKEN": "..."
      }
    },
    "filesystem": {
      "type": "stdio",
      "command": "node",
      "args": ["path/to/server-filesystem"],
      "env": {}
    },
    "memory-bank": {
      "type": "stdio",
      "command": "server-memory",
      "args": [],
      "env": {}
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "code-reasoning",
      "args": [],
      "env": {}
    },
    "brave-search": {
      "type": "stdio",
      "command": "server-brave-search",
      "args": [],
      "env": {}
    },
    "google-maps": {
      "type": "stdio",
      "command": "server-google-maps",
      "args": [],
      "env": {
        "GOOGLE_MAPS_API_KEY": "..."
      }
    },
    "deep-graph": {
      "type": "stdio",
      "command": "mcp-code-graph",
      "args": [],
      "env": {}
    },
    "task-master-ai": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "--package=task-master-ai", "task-master-ai"],
      "env": {
        "ANTHROPIC_API_KEY": "YOUR_ANTHROPIC_API_KEY_HERE",
        "PERPLEXITY_API_KEY": "YOUR_PERPLEXITY_API_KEY_HERE",
        "OPENAI_API_KEY": "YOUR_OPENAI_KEY_HERE",
        "GOOGLE_API_KEY": "YOUR_GOOGLE_KEY_HERE",
        "XAI_API_KEY": "YOUR_XAI_KEY_HERE",
        "OPENROUTER_API_KEY": "YOUR_OPENROUTER_KEY_HERE",
        "MISTRAL_API_KEY": "YOUR_MISTRAL_KEY_HERE",
        "AZURE_OPENAI_API_KEY": "YOUR_AZURE_KEY_HERE",
        "OLLAMA_API_KEY": "YOUR_OLLAMA_API_KEY_HERE"
      }
    }
  }
}
